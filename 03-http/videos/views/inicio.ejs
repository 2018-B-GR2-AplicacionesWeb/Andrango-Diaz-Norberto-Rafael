<style>
    table, th, td{
        border: 1px solid black;
    }
</style>

<% var apellido = 'r'%>
<!-- asdasd-->

<h1><%= nombre%><%= apellido%></h1>

<p>Parrafo</p>
<p>Arreglo</p>


<ol>
    <li>1</li>
    <li>2</li>
    <li>3</li>

</ol>

<ul>

<% arreglo.forEach((numero)=>{ %>
        <li><%= numero%></li>
<%}) %>

</ul>


<table>
    <!--tr ->table row -->
    <tr>
        <th>Identificador</th>
        <th>Nombre</th>
        <th>Acciones</th>
    </tr>
    <% arreglo.forEach((usuario)=>{ %>
        <tr>
            <td><%usuario.id%></td>
            <td><%usuario.nombre%></td>
            <td>
                <button>Eliminar</button>
            </td>

        </tr>

    <%})%>

    <tr>
        <td>1</td>
        <td>Raf</td>
        <td>
            <form action="/Usuario/borrar/<%usuario.id%>"
                method="post";
                  >
                <button>Eliminar</button>
            </form>

        </td>
    </tr>
</table>